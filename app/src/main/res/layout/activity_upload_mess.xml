<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UploadMessActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_above="@id/upl_mess_upload">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <!-- Title -->
                <TextView
                    android:id="@+id/tv_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="@dimen/_30sdp"
                    android:text="Register Mess"
                    android:textColor="@color/dark_purple"
                    android:textSize="30sp"
                    android:textStyle="bold" />

                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/upl_room_name_layout"
                    android:layout_width="@dimen/_254sdp"
                    android:layout_height="@dimen/_57sdp"
                    android:layout_below="@id/tv_title"
                    android:layout_centerHorizontal="true"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_15sdp"
                    android:layout_marginTop="@dimen/_12sdp"
                    android:layout_marginEnd="@dimen/_15sdp"
                    android:hint="Mess Name"
                    app:boxBackgroundColor="@color/lightpurple"
                    app:boxCornerRadiusBottomEnd="@dimen/_15sdp"
                    app:boxCornerRadiusBottomStart="@dimen/_15sdp"
                    app:boxCornerRadiusTopEnd="@dimen/_15sdp"
                    app:boxCornerRadiusTopStart="@dimen/_15sdp"
                    app:boxStrokeColor="@color/dark_purple"
                    app:boxStrokeWidth="@dimen/_2sdp"
                    app:hintTextColor="@color/dark_purple">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/upl_mess_name"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_50sdp"
                        android:layout_gravity="center"
                        android:layout_marginTop="@dimen/_6sdp" />

                </com.google.android.material.textfield.TextInputLayout>

                <Spinner
                    android:id="@+id/mess_type"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/upl_room_name_layout"
                    android:layout_alignStart="@id/upl_room_name_layout"
                    android:layout_marginTop="@dimen/_10sdp"/>

                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/upl_room_location_layout"
                    android:layout_width="@dimen/_254sdp"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/mess_type"
                    android:layout_centerHorizontal="true"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_15sdp"
                    android:layout_marginTop="@dimen/_12sdp"
                    android:layout_marginEnd="@dimen/_15sdp"
                    android:hint="Mess Location"
                    android:minHeight="@dimen/_57sdp"
                    app:boxBackgroundColor="@color/lightpurple"
                    app:boxCornerRadiusBottomEnd="@dimen/_15sdp"
                    app:boxCornerRadiusBottomStart="@dimen/_15sdp"
                    app:boxCornerRadiusTopEnd="@dimen/_15sdp"
                    app:boxCornerRadiusTopStart="@dimen/_15sdp"
                    app:boxStrokeColor="@color/dark_purple"
                    app:boxStrokeWidth="@dimen/_2sdp"
                    app:hintTextColor="@color/dark_purple">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/upl_mess_location"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="@dimen/_6sdp"
                        android:inputType="textMultiLine"
                        android:maxLines="5"
                        android:minHeight="@dimen/_50sdp" />

                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/upl_room_price_layout"
                    android:layout_width="@dimen/_254sdp"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/upl_room_location_layout"
                    android:layout_centerHorizontal="true"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_15sdp"
                    android:layout_marginTop="@dimen/_12sdp"
                    android:layout_marginEnd="@dimen/_15sdp"
                    android:hint="Mess Price"
                    android:minHeight="@dimen/_57sdp"
                    app:boxBackgroundColor="@color/lightpurple"
                    app:boxCornerRadiusBottomEnd="@dimen/_15sdp"
                    app:boxCornerRadiusBottomStart="@dimen/_15sdp"
                    app:boxCornerRadiusTopEnd="@dimen/_15sdp"
                    app:boxCornerRadiusTopStart="@dimen/_15sdp"
                    app:boxStrokeColor="@color/dark_purple"
                    app:boxStrokeWidth="@dimen/_2sdp"
                    app:hintTextColor="@color/dark_purple">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/upl_mess_price"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="@dimen/_6sdp"
                        android:inputType="textMultiLine"
                        android:maxLines="5"
                        android:minHeight="@dimen/_50sdp" />

                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/upl_room_description_layout"
                    android:layout_width="@dimen/_254sdp"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/upl_room_price_layout"
                    android:layout_centerHorizontal="true"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_15sdp"
                    android:layout_marginTop="@dimen/_12sdp"
                    android:layout_marginEnd="@dimen/_15sdp"
                    android:hint="Mess Description"
                    android:minHeight="@dimen/_57sdp"
                    app:boxBackgroundColor="@color/lightpurple"
                    app:boxCornerRadiusBottomEnd="@dimen/_15sdp"
                    app:boxCornerRadiusBottomStart="@dimen/_15sdp"
                    app:boxCornerRadiusTopEnd="@dimen/_15sdp"
                    app:boxCornerRadiusTopStart="@dimen/_15sdp"
                    app:boxStrokeColor="@color/dark_purple"
                    app:boxStrokeWidth="@dimen/_2sdp"
                    app:hintTextColor="@color/dark_purple">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/upl_facility"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="@dimen/_6sdp"
                        android:inputType="textMultiLine"
                        android:maxLines="5"
                        android:minHeight="@dimen/_50sdp" />

                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/roomFacilities"
                    android:layout_width="@dimen/_254sdp"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="@dimen/_12sdp"
                    app:cardCornerRadius="20dp"
                    app:strokeColor="@color/purple"
                    app:strokeWidth="2dp"
                    app:cardElevation="4dp"
                    app:cardBackgroundColor="@color/lightpurple"
                    android:layout_below="@id/upl_room_description_layout">

                    <GridLayout
                        android:id="@+id/grid"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:columnCount="3"
                        android:padding="@dimen/_8sdp">

                        <CheckBox
                            android:id="@+id/CheckBox1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/_6sdp"
                            android:textColor="@color/black"
                            android:textStyle="bold"
                            android:text="AC" />

                        <CheckBox
                            android:id="@+id/CheckBox2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/_6sdp"
                            android:textColor="@color/black"
                            android:textStyle="bold"
                            android:text="Drinking Water" />

                        <CheckBox
                            android:id="@+id/CheckBox3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/_6sdp"
                            android:textColor="@color/black"
                            android:textStyle="bold"
                            android:text="Parcel" />

<!--                        <CheckBox-->
<!--                            android:id="@+id/CheckBox4"-->
<!--                            android:layout_width="wrap_content"-->
<!--                            android:layout_height="wrap_content"-->
<!--                            android:layout_margin="@dimen/_6sdp"-->
<!--                            android:textColor="@color/black"-->
<!--                            android:textStyle="bold"-->
<!--                            android:text="AC" />-->

<!--                        <CheckBox-->
<!--                            android:id="@+id/CheckBox5"-->
<!--                            android:layout_width="wrap_content"-->
<!--                            android:layout_height="wrap_content"-->
<!--                            android:layout_margin="@dimen/_6sdp"-->
<!--                            android:textColor="@color/black"-->
<!--                            android:textStyle="bold"-->
<!--                            android:text="Cable" />-->

<!--                        <CheckBox-->
<!--                            android:id="@+id/CheckBox6"-->
<!--                            android:layout_width="wrap_content"-->
<!--                            android:layout_height="wrap_content"-->
<!--                            android:layout_margin="@dimen/_6sdp"-->
<!--                            android:textColor="@color/black"-->
<!--                            android:textStyle="bold"-->
<!--                            android:text="Gas Line" />-->

                    </GridLayout>

                </com.google.android.material.card.MaterialCardView>

                <!--            <com.google.android.material.textfield.TextInputLayout-->
                <!--                android:id="@+id/upl_facility_layout"-->
                <!--                android:layout_width="@dimen/_254sdp"-->
                <!--                android:layout_height="wrap_content"-->
                <!--                android:layout_below="@id/upl_room_price_layout"-->
                <!--                android:layout_centerHorizontal="true"-->
                <!--                android:layout_gravity="center"-->
                <!--                android:layout_marginStart="@dimen/_15sdp"-->
                <!--                android:layout_marginTop="@dimen/_12sdp"-->
                <!--                android:layout_marginEnd="@dimen/_15sdp"-->
                <!--                android:hint="Room Facilities"-->
                <!--                android:minHeight="@dimen/_57sdp"-->
