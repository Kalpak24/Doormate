                <!--                    android:maxLines="5"-->
                <!--                    android:minHeight="@dimen/_50sdp" />-->

                <!--            </com.google.android.material.textfield.TextInputLayout>-->

                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/upl_owner_name_layout"
                    android:layout_width="@dimen/_254sdp"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/roomFacilities"
                    android:layout_centerHorizontal="true"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_15sdp"
                    android:layout_marginTop="@dimen/_12sdp"
                    android:layout_marginEnd="@dimen/_15sdp"
                    android:hint="Owner Name"
                    android:minHeight="@dimen/_57sdp"
                    app:boxBackgroundColor="@color/lightpurple"
                    app:boxCornerRadiusBottomEnd="@dimen/_15sdp"
                    app:boxCornerRadiusBottomStart="@dimen/_15sdp"
                    app:boxCornerRadiusTopEnd="@dimen/_15sdp"
                    app:boxCornerRadiusTopStart="@dimen/_15sdp"
                    app:boxStrokeColor="@color/dark_purple"
                    app:boxStrokeWidth="@dimen/_2sdp"
                    app:hintTextColor="@color/dark_purple">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/upl_owner_name"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="@dimen/_6sdp"
                        android:inputType="textMultiLine"
                        android:maxLines="5"
                        android:minHeight="@dimen/_50sdp" />

                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/upl_owner_contact_layout"
                    android:layout_width="@dimen/_254sdp"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/upl_owner_name_layout"
                    android:layout_centerHorizontal="true"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_15sdp"
                    android:layout_marginTop="@dimen/_12sdp"
                    android:layout_marginEnd="@dimen/_15sdp"
                    android:hint="Owner Contact"
                    android:minHeight="@dimen/_57sdp"
                    app:boxBackgroundColor="@color/lightpurple"
                    app:boxCornerRadiusBottomEnd="@dimen/_15sdp"
                    app:boxCornerRadiusBottomStart="@dimen/_15sdp"
                    app:boxCornerRadiusTopEnd="@dimen/_15sdp"
                    app:boxCornerRadiusTopStart="@dimen/_15sdp"
                    app:boxStrokeColor="@color/dark_purple"
                    app:boxStrokeWidth="@dimen/_2sdp"
                    app:hintTextColor="@color/dark_purple">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/upl_owner_contact"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="@dimen/_6sdp"
                        android:inputType="textMultiLine"
                        android:maxLines="5"
                        android:minHeight="@dimen/_50sdp" />

                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/open_time_layout"
                    android:layout_width="@dimen/_254sdp"
                    android:layout_height="@dimen/_110sdp"
                    android:layout_below="@id/upl_owner_contact_layout"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="@dimen/_12sdp"
                    app:cardBackgroundColor="@color/lightpurple"
                    app:cardCornerRadius="20dp"
                    app:strokeColor="@color/primaryMedium"
                    app:strokeWidth="2dp">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/tv1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Open Time"
                            android:textSize="16sp"
                            android:textColor="@color/dark_purple"
                            android:layout_alignParentTop="true"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="5dp"/>

                        <TimePicker
                            android:id="@+id/mess_open_time"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_80sdp"
                            android:timePickerMode="spinner"
                            android:layout_centerInParent="true"/>

                    </RelativeLayout>

                </com.google.android.material.card.MaterialCardView>

                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/close_time_layout"
                    android:layout_width="@dimen/_254sdp"
                    android:layout_height="@dimen/_110sdp"
                    android:layout_below="@id/open_time_layout"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="@dimen/_12sdp"
                    app:cardBackgroundColor="@color/lightpurple"
                    app:cardCornerRadius="20dp"
                    app:strokeColor="@color/dark_purple"
                    app:strokeWidth="2dp">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/tv2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Close Time"
                            android:textColor="@color/dark_purple"
                            android:layout_alignParentTop="true"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="5dp"/>

                        <TimePicker
                            android:id="@+id/mess_close_time"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_80sdp"
                            android:timePickerMode="spinner"
                            android:layout_centerInParent="true"/>

                    </RelativeLayout>

                </com.google.android.material.card.MaterialCardView>

                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/room_relative"
                    android:layout_width="@dimen/_110sdp"
                    android:layout_height="@dimen/_110sdp"
                    android:layout_marginTop="@dimen/_12sdp"
                    android:layout_below="@id/close_time_layout"
                    android:layout_centerHorizontal="true"
                    app:cardBackgroundColor="@color/white">

                    <ImageView
                        android:id="@+id/upl_mess_image"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:src="@drawable/upload_image" />

                    <ImageView
                        android:id="@+id/upl_mess_load"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_centerHorizontal="true"/>

                </com.google.android.material.card.MaterialCardView>

            </RelativeLayout>

        </ScrollView>

        <!-- Submit Button -->
        <com.google.android.material.button.MaterialButton
            android:id="@+id/upl_mess_upload"
            android:layout_width="@dimen/_282sdp"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/_5sdp"
            android:layout_marginBottom="@dimen/_8sdp"
            android:backgroundTint="@color/dark_purple"
            android:padding="@dimen/_10sdp"
            android:text="Register"
            android:textColor="@color/primaryLight"
            android:textSize="16sp"
            app:icon="@drawable/baseline_register_24"
            app:iconSize="22dp"
            app:iconGravity="textStart"/>

    </RelativeLayout>




